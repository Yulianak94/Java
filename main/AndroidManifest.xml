<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vaadapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VaadApp">
        <activity android:name=".Activities.BuilldingCasherActivity"  android:theme="@style/Theme.VaadApp.NoActionBar"></activity>
        <activity
            android:name=".Activities.UserPayments"
            android:theme="@style/Theme.VaadApp.NoActionBar" />
        <activity
            android:name=".Activities.NewPaymentActivity"
            android:theme="@style/Theme.VaadApp.NoActionBar" />
        <activity
            android:name=".Activities.PaymentsActivity"
            android:theme="@style/Theme.VaadApp.NoActionBar" />
        <activity
            android:name=".Activities.ApartmentsActivity"
            android:theme="@style/Theme.VaadApp.NoActionBar" />
        <activity
            android:name=".Activities.UserRegisterActivity"
            android:theme="@style/Theme.VaadApp.NoActionBar" />
        <activity
            android:name=".Activities.AuthActivity"
            android:theme="@style/Theme.VaadApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.MainActivity"
            android:theme="@style/Theme.VaadApp.NoActionBar" />
    </application>

</manifest>